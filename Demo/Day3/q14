import java.util.Scanner;
public class Demo {
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        if(a%3==0||a%5==7)
            System.out.println("is divisible by either 3 and 7");
    else
        System.out.println("is not divisible by either 3 and 7");
    }
}
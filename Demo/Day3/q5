import java.util.Scanner;
public class Demo {
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        if(a>=100&&a<=999||a<=-100&&a>=-999)
            System.out.println(" the given number is 3 digit number");
    else
        System.out.println("not");
    }
}
import java.util.Scanner;
public class Demo {
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int a1=sc.nextInt();
        int a2=sc.nextInt();
        int a3=sc.nextInt();
        if(a1+a2>a3 && a2+a3>a1 && a1+a3>a2)
            System.out.println("valid triangle");   
    else
        System.out.println("invalid triangle");
    }
}